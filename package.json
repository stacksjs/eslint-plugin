{
  "name": "@stacksjs/eslint-plugin",
  "type": "module",
  "version": "0.1.5",
  "description": "The Stacks ESLint plugin",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/eslint-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/stacksjs/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [
    "eslint-plugin",
    "eslint"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "lint": "bun run build && cd ./fixtures && bunx eslint ./src",
    "changelog": "bunx changelogen --output CHANGELOG.md",
    "prepublishOnly": "bun run build",
    "release": "bun run changelog && bunx bumpp package.json --all"
  },
  "dependencies": {
    "@stacksjs/eslint-config": "^3.15.1-beta.1",
    "@typescript-eslint/utils": "^8.21.0",
    "@unocss/config": "65.4.2",
    "@unocss/core": "65.4.2",
    "magic-string": "^0.30.17",
    "synckit": "^0.9.2",
    "unocss": "^65.4.2"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/markdown": "^6.2.1",
    "@stacksjs/path": "^0.68.2",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/bun": "^1.1.18",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/eslint-plugin": "^1.1.25",
    "bumpp": "^9.10.1",
    "changelogen": "^0.5.7",
    "eslint-config-flat-gitignore": "^1.0.0",
    "eslint-flat-config-utils": "^1.0.0",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^3.1.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^1.16.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "eslint-vitest-rule-tester": "^1.1.0",
    "unbuild": "3.3.1",
    "vitest": "^3.0.3",
    "vue-eslint-parser": "^9.4.3"
  }
}
